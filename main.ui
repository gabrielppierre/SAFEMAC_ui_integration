/* /////////////////////////////////////////////////////////////////////////////////////////////////
Tooltip */
QToolTip {
    color: #ffffff;
    background-color: blue;
    border: none;
	border-left: 5px solid yellow;
    padding: 5px;
    font-size: 10px;
    font-weight: bold;
    text-align: center;
}

QToolTip::text {
    background-color: #3a8ab0;
    padding: 4px;
    border-radius: 4px;
}


